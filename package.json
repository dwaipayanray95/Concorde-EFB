{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "../dist",
    "distDir": "../dist"
  },
  "package": {
    "productName": "concorde-efb",
    "version": "0.0.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "category": "Utility",
      "identifier": "io.github.dwaipayanray95.concordeefb",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png"
      ],
      "resources": [],
      "targets": [
        "all"
      ],
      "windows": [
        {
          "certificateThumbprint": null,
          "digestAlgorithm": "sha256",
          "timestampUrl": "http://timestamp.digicert.com"
        }
      ]
    },
    "allowlist": {
      "all": true
    },
    "updater": {
      "active": false,
      "dialog": false,
      "endpoints": []
    },
    "windows": [
      {
        "title": "concorde-efb",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  }
}